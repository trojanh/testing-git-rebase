<nav class="tab-bar">
  <section class="left-small">
    {{link-to (t "back") 'delivery.contact_details' classNames="back"}}
  </section>

  <section class="middle tab-bar-section">
    <h1 class="title">{{t "thank_you"}}</h1>
  </section>
</nav>

<section class="main-section ui contact_details">
  <div class="row">
    <div class="small-12 columns">
      {{t "delivery.thank_offer.staff_will_contact" name=contact.name mobile=contact.mobile}}
    </div>
  </div>
</section>

<div class="btm">
  <div class="row">
    <div class="small-10 small-offset-1 columns">
      <button {{action 'done'}} class="button expand submitForm">{{t "contact.done"}}</button>
    </div>
  </div>
</div>
